{
  "expo": {
    "name": "Painel AimlessFF",
    "slug": "painel-aimlessff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/react-logo@3x.png",
    "scheme": "painelaimlessff",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/react-logo@3x.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "SYSTEM_ALERT_WINDOW",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "package": "com.aimlessff.painel"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
